{
  "id": "yt",
  "name": "YT Stream",
  "abbrev": "YT",
  "version": "0.1.1",
  "description": "Experimental YouTube audio stream generator using yt-dlp + ffmpeg",
  "author": "charlesvestal",
  "license": "MIT",
  "ui": "ui.js",
  "ui_chain": "ui.js",
  "dsp": "dsp.so",
  "api_version": 2,
  "capabilities": {
    "audio_out": true,
    "audio_in": false,
    "midi_in": false,
    "midi_out": false,
    "chainable": true,
    "component_type": "sound_generator",
    "chain_params": [
      {
        "key": "play_pause_step",
        "name": "Play/Pause",
        "type": "enum",
        "options": ["idle", "trigger"],
        "default": "idle"
      },
      {
        "key": "rewind_15_step",
        "name": "Rewind 15s",
        "type": "enum",
        "options": ["idle", "trigger"],
        "default": "idle"
      },
      {
        "key": "forward_15_step",
        "name": "Forward 15s",
        "type": "enum",
        "options": ["idle", "trigger"],
        "default": "idle"
      },
      {
        "key": "gain",
        "name": "Gain",
        "type": "float",
        "min": 0.0,
        "max": 2.0,
        "default": 1.0,
        "step": 0.05
      },
      {
        "key": "gain",
        "name": "Gain",
        "type": "float",
        "min": 0.0,
        "max": 2.0,
        "default": 1.0,
        "step": 0.05
      },
      {
        "key": "gain",
        "name": "Gain",
        "type": "float",
        "min": 0.0,
        "max": 2.0,
        "default": 1.0,
        "step": 0.05
      },
      {
        "key": "stop_step",
        "name": "Stop",
        "type": "enum",
        "options": ["idle", "trigger"],
        "default": "idle"
      },
      {
        "key": "restart_step",
        "name": "Restart",
        "type": "enum",
        "options": ["idle", "trigger"],
        "default": "idle"
      }
    ]
  },
  "defaults": {
    "gain": 1.0
  }
}
